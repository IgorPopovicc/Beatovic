{
  "version": 2,
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "dist/Beatovic/server/server.mjs" }
  ],
  "functions": {
    "api/ssr.ts": { "runtime": "nodejs20.x" }
  },
  "rewrites": [
    { "source": "/api/:path*", "destination": "https://euroleague-tiebreaker.com/api/:path*" }
  ]
}
